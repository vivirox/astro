# Example environment variables (replace with your actual values in .env.local)
AXIOM_DATASET=your_dataset_name
AXIOM_TOKEN=your_token_here
CONVEX_URL=https://your-deployment-id.convex.cloud
EMAIL_FROM=your_email@example.com
ENABLE_RATE_LIMITING=true
LOG_LEVEL=debug
MODAL_TOKEN_ID=your_token_id_here
MODAL_TOKEN_SECRET=your_token_secret_here
OPENAI_API_KEY=your_api_key_here
OPENAI_BASE_URL=https://api.example.com/v1
PORT=3000
POSTGRES_PASSWORD=your_password_here
POSTGRES_PRISMA_URL=postgres://user:password@host:port/db?sslmode=require
POSTGRES_URL=postgres://user:password@host:port/db?sslmode=require
POSTGRES_URL_NON_POOLING=postgres://user:password@host:port/db?sslmode=require
RATE_LIMIT_MAX_REQUESTS=100
RATE_LIMIT_WINDOW_MS=900000
REPLICATE_API_TOKEN=your_token_here
RESEND_API_KEY=your_key_here
SENTRY_DSN=https://your_sentry_dsn_here
SUPABASE_ANON_KEY=your-anon-key
SUPABASE_JWT_SECRET=your_jwt_secret_here
SUPABASE_KEY=your_key_here
SUPABASE_SERVICE_ROLE_KEY=your-service-role-key
SUPABASE_URL=https://your-project-id.supabase.co
TOGETHER_API_KEY=your_api_key_here
VITE_API_URL=https://your-api-url/api
VITE_SUPABASE_ANON_KEY=your_anon_key_here
VITE_SUPABASE_URL=https://your-project.supabase.co
GOOGLE_API_KEY=your_api_key_here
VERCEL_TOKEN=your_token_here
VERCEL_ORG_ID=your_org_id_here
VERCEL_PROJECT_ID=your_project_id_here
VITE_LITLYX_PROJECT_ID=your_project_id_here
VITE_LITLYX_API_KEY=your_api_key_here

# Redis Configuration - Required in production
UPSTASH_REDIS_REST_URL=https://your-region.upstash.io/redis/your-database-id
UPSTASH_REDIS_REST_TOKEN=your-upstash-token
REDIS_URL=redis://username:password@host:port
REDIS_USERNAME=your_redis_username
REDIS_PASSWORD=your_redis_password
REDIS_HOST=your_redis_host
REDIS_PORT=6379
REDIS_DATABASE=0
REDIS_TLS_ENABLED=true
REDIS_CACHE_TTL=3600

# Logging configuration
LOG_CONSOLE=true
LOG_AUDIT=true

# Security configuration
SECURITY_ENABLE_BRUTE_FORCE_PROTECTION=true
SECURITY_MAX_LOGIN_ATTEMPTS=5
SECURITY_ACCOUNT_LOCKOUT_DURATION=1800
SECURITY_API_ABUSE_THRESHOLD=100
SECURITY_ENABLE_ALERTS=true

# Monitoring Configuration
GRAFANA_URL=https://grafana.gradiantascent.com
GRAFANA_API_KEY=your-grafana-api-key
GRAFANA_ORG_ID=your-grafana-org-id
MONITORING_SLACK_WEBHOOK=your-slack-webhook-url
MONITORING_EMAIL_RECIPIENTS=admin@example.com,alerts@example.com
APP_VERSION=1.0.0

# Backup Configuration
BACKUP_STORAGE_PROVIDER=s3
BACKUP_STORAGE_BUCKET=gradiant-backups
BACKUP_STORAGE_PREFIX=prod
BACKUP_STORAGE_REGION=us-east-1
BACKUP_SLACK_WEBHOOK=your-slack-webhook-url
BACKUP_EMAIL_RECIPIENTS=admin@example.com,alerts@example.com

# Other Configuration
NODE_ENV=development

# Sentry Error Tracking
PUBLIC_SENTRY_DSN=https://xxxxxxxxxxxxxxxxxxxxxxxxx.ingest.sentry.io/xxxxxxxx
PUBLIC_SENTRY_ENVIRONMENT=development
PUBLIC_SENTRY_RELEASE=1.0.0
# Required for source map uploads - create at https://sentry.io/settings/account/api/auth-tokens/
# with project:releases and project:read scopes
SENTRY_AUTH_TOKEN=your_sentry_auth_token

# ELK Stack (Elasticsearch, Logstash, Kibana) Configuration
ELK_ENABLED=true
ELK_URL=http://elasticsearch:9200
ELK_INDEX_PREFIX=gradiant-ascent
ELK_USERNAME=elastic
ELK_PASSWORD=your-elasticsearch-password
ELK_API_KEY=your-elasticsearch-api-key
ELK_NODE_NAME=astro-server
