<BaseLayout
  title={frontmatter.title}
  description={frontmatter.description}
  bgType={frontmatter.bgType}
  ogImage={frontmatter.ogImage}
>
  <StandardLayout isCentered={true}>
    <Fragment slot="head">
      <h1 class="ml-12 lt-sm:ml-0">
        AstroEco is <span class="animate-pulse">Contributing...</span>
      </h1>
      <p class="mt--4! op-50 italic">
        Example: Displaying GitHub pull requests using <Link
          href="https://github.com/lin-stephanie/astro-loaders/tree/main/packages/astro-loader-github-prs"
          external={true}>astro-loader-github-prs</Link
        >
      </p>
    </Fragment>
    <GithubView slot="github" collectionType="prs" />
  </StandardLayout>
</BaseLayout>
