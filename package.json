{
  "name": "gradiant-astro",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.7.1",
  "license": "MIT",
  "engines": {
    "node": "22.x"
  },
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "dev": "astro dev --host",
    "start": "astro dev",
    "check": "trunk check",
    "prebuild": "node scripts/validate-env.js",
    "build": "astro build",
    "build:prod": "astro build && node scripts/optimize-css.js",
    "preview": "astro preview",
    "format": "prettier --write .",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,astro}\"",
    "lint": "prettier --write  \"**/*.{js,jsx,ts,tsx,md,mdx,astro}\" && eslint --fix \"src/**/*.{js,ts,jsx,tsx,astro}\"",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint:types": "tsc --noEmit",
    "lint:staged": "lint-staged",
    "check:all": "pnpm run format:check && pnpm run lint:check && pnpm run lint:types",
    "toolbar:on": "astro preferences enable devToolbar",
    "toolbar:off": "astro preferences disable devToolbar",
    "postbuild": "tsx scripts/cleanup-search.ts",
    "trunk": "trunk",
    "fmt": "trunk fmt",
    "test": "vitest",
    "test:watch": "vitest --watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:unit": "vitest run src/__tests__/**/*.test.ts",
    "test:integration": "vitest run src/__tests__/**/*.integration.test.ts",
    "test:e2e": "playwright test",
    "test:setup": "tsx src/scripts/run-tests.ts --setup-only",
    "test:a11y": "vitest run src/**/*.a11y.test.ts",
    "test:a11y:watch": "vitest watch src/**/*.a11y.test.ts",
    "test:a11y:e2e": "playwright test tests/accessibility",
    "test:a11y:all": "pnpm run test:a11y && pnpm run test:a11y:e2e",
    "lint:strict": "pnpm run format:check && pnpm run lint:check && pnpm run lint:types",
    "fix": "tsx scripts/fix-lint.ts",
    "fix:lint": "eslint --fix",
    "fix:format": "prettier --write",
    "fix:all": "pnpm run fix:format && pnpm run fix:lint",
    "fix:jsx": "tsx scripts/fix-jsx-tags.ts",
    "worker:email": "tsx src/workers/email-worker.ts",
    "worker:email:dev": "tsx watch src/workers/email-worker.ts",
    "worker:notification": "tsx src/workers/notification-worker.ts",
    "worker:notification:dev": "tsx watch src/workers/notification-worker.ts",
    "worker:analytics": "tsx src/workers/analytics-worker.ts",
    "worker:analytics:dev": "tsx watch src/workers/analytics-worker.ts",
    "test:redis": "vitest --config src/lib/services/redis/__tests__/vitest.config.ts",
    "test:redis:unit": "vitest run --config src/lib/services/redis/__tests__/vitest.config.ts '**/RedisService.test.ts'",
    "test:redis:integration": "vitest run --config src/lib/services/redis/__tests__/vitest.config.ts '**/RedisService.integration.test.ts'",
    "test:redis:perf": "vitest run --config src/lib/services/redis/__tests__/vitest.config.ts '**/RedisService.perf.test.ts'",
    "test:redis:coverage": "vitest run --coverage --config src/lib/services/redis/__tests__/vitest.config.ts",
    "test:redis:watch": "vitest --config src/lib/services/redis/__tests__/vitest.config.ts",
    "test:redis:all": "tsx src/lib/services/redis/__tests__/run-tests.ts",
    "test:integration:playwright": "playwright test tests/integration",
    "test:performance": "playwright test tests/performance",
    "test:perf:run": "tsx scripts/run-performance-tests.ts",
    "provision:dashboard": "tsx scripts/provision-grafana-dashboard.ts",
    "deploy:staging": "bash scripts/deploy.sh staging",
    "deploy:production": "bash scripts/deploy.sh production",
    "deploy:stop": "tsx scripts/stop-deploy.ts",
    "deploy:rollback": "tsx scripts/rollback.ts",
    "analyze": "ANALYZE=true npm run build",
    "eslint:dump": "eslint --print-config ./index.js > eslint-dump.js",
    "postinstall": "node scripts/check-env.js",
    "setup:env": "tsx scripts/setup-env.ts",
    "diagnostics": "tsx scripts/diagnostics.ts",
    "deploy:convex": "ts-node scripts/deploy-convex.ts",
    "deploy:convex:prod": "NODE_ENV=production ts-node scripts/deploy-convex.ts",
    "test:convex": "vitest run convex/**/*.test.ts",
    "test:convex:watch": "vitest watch convex/**/*.test.ts",
    "test:convex:coverage": "vitest run convex/**/*.test.ts --coverage"
  },
