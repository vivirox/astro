{
  "name": "gradiant-astro",
  "type": "module",
  "version": "0.0.1",
  "private": true,
  "packageManager": "pnpm@10.6.1",
  "license": "MIT",
  "engines": {
    "node": ">=20.0.0"
  },
  "scripts": {
    "astro": "astro",
    "sync": "astro sync",
    "dev": "astro dev --open",
    "check": "trunk check",
    "build": "astro build",
    "preview": "astro preview",
    "format": "prettier --write \"src/**/*.{ts,tsx,astro}\"",
    "format:check": "prettier --check \"src/**/*.{ts,tsx,astro}\"",
    "lint": "pnpm run format && pnpm run lint:fix && pnpm run lint:types",
    "lint:check": "eslint \"src/**/*.{ts,tsx}\"",
    "lint:fix": "eslint --fix \"src/**/*.{ts,tsx}\"",
    "lint:types": "tsc --noEmit",
    "lint:staged": "lint-staged",
    "check:all": "pnpm run format:check && pnpm run lint:check && pnpm run lint:types",
    "toolbar:on": "astro preferences enable devToolbar",
    "toolbar:off": "astro preferences disable devToolbar",
    "prepare": "simple-git-hooks",
    "postbuild": "pagefind --site dist || true && del-cli \"dist/pagefind/*ui*\" || true",
    "trunk": "trunk",
    "fmt": "trunk fmt",
    "test": "vitest",
    "test:watch": "vitest watch",
    "test:ui": "vitest --ui",
    "test:coverage": "vitest run --coverage",
    "test:e2e": "playwright test",
    "test:setup": "tsx src/scripts/run-tests.ts --setup-only",
    "lint:strict": "pnpm run format:check && pnpm run lint:check && pnpm run lint:types",
    "fix": "tsx scripts/fix-lint.ts",
    "fix:lint": "eslint --fix",
    "fix:format": "prettier --write",
    "fix:all": "pnpm run fix:format && pnpm run fix:lint",
    "fix:jsx": "tsx scripts/fix-jsx-tags.ts",
    "worker:email": "tsx src/workers/email-worker.ts",
    "worker:email:dev": "tsx watch src/workers/email-worker.ts",
    "worker:notification": "tsx src/workers/notification-worker.ts",
    "worker:notification:dev": "tsx watch src/workers/notification-worker.ts",
    "worker:analytics": "tsx src/workers/analytics-worker.ts",
    "worker:analytics:dev": "tsx watch src/workers/analytics-worker.ts",
    "test:redis": "jest --config src/lib/services/redis/__tests__/jest.config.ts",
    "test:redis:unit": "jest --config src/lib/services/redis/__tests__/jest.config.ts --testMatch '**/RedisService.test.ts'",
    "test:redis:integration": "jest --config src/lib/services/redis/__tests__/jest.config.ts --testMatch '**/RedisService.integration.test.ts'",
    "test:redis:perf": "jest --config src/lib/services/redis/__tests__/jest.config.ts --testMatch '**/RedisService.perf.test.ts'",
    "test:redis:coverage": "jest --config src/lib/services/redis/__tests__/jest.config.ts --coverage",
    "test:redis:watch": "jest --config src/lib/services/redis/__tests__/jest.config.ts --watch",
    "test:redis:all": "tsx src/lib/services/redis/__tests__/run-tests.ts",
    "provision:dashboard": "ts-node scripts/provision-grafana-dashboard.ts",
    "deploy:staging": "ts-node scripts/run-deploy.ts staging",
    "deploy:production": "ts-node scripts/run-deploy.ts production",
    "deploy:stop": "ts-node scripts/stop-deploy.ts",
    "deploy:rollback": "ts-node scripts/rollback.ts"
  },
